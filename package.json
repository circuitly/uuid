{
  "version": "1.0.1",
  "name": "@lukeed/uuid",
  "repository": "lukeed/uuid",
  "description": "A tiny (230B), fast, and cryptographically secure UUID (v4) generator for Node and the browser",
  "browser": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "umd:name": "uuid",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "bundt",
    "pretest": "npm run build",
    "test": "uvu -r esm test"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "modes": {
    "secure": "src/secure.js"
  },
  "keywords": [
    "guid",
    "browser",
    "isomorphic",
    "rfc4122",
    "random",
    "uuid",
    "uid"
  ],
  "dependencies": {
    "@lukeed/csprng": "^1.0.0"
  },
  "devDependencies": {
    "bundt": "1.1.1",
    "esm": "3.2.25",
    "is-uuid": "1.0.2",
    "uvu": "0.3.4"
  }
}
